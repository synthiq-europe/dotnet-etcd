    <Authors>Synthiq</Authors>
    <Company>Synthiq</Company>
    <Copyright>Copyright (C) 2025 Synthiq</Copyright>
    <Description>gRPC client for interacting with the etcd API from .NET.</Description>

    <PackageIcon>icon.png</PackageIcon>
    <PackageId>Synthiq.Etcd.Api</PackageId>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/synthiq-europe/dotnet-etcd</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>etcd;grpc</PackageTags>

    <RepositoryType>git</RepositoryType>
